"  connect <ifname>\n"
"\n"
"  reapply <ifname>\n"
"\n"
"  modify <ifname> ([+|-]<setting>.<property> <value>)+\n"
"\n"
"  disconnect <ifname> ...\n"
"\n"
"  delete <ifname> ...\n"
"\n"
