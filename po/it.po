"                  [mtu <MTU>]\n"
"                  [mac <MAC address>]\n"
"\n"
"    gsm:          apn <APN>\n"
"                  [user <username>]\n"
"                  [password <password>]\n"
"\n"
"    cdma:         [user <username>]\n"
"                  [password <password>]\n"
"\n"
"    infiniband:   [mac <MAC address>]\n"
"                  [mtu <MTU>]\n"
"                  [transport-mode datagram | connected]\n"
"                  [parent <ifname>]\n"
"                  [p-key <IPoIB P_Key>]\n"
